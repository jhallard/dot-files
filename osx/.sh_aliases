alias srcz="source ~/.zshrc"
alias edz="vim ~/.zshrc"
alias eda="vim ~/.sh_aliases && srcz"
alias edf="vim ~/.sh_functions && srcz"
alias vim="nvim"

alias gitaa="git add -A :/"
alias gitc="git commit"
alias gitpom="git push -u origin master"
alias gitp="git push -u"
alias goconf="cd ~/.config"
alias gonvim="cd ~/.config/nvim/lua/"
alias go="cd ~/workspace"
alias lg="lazygit"
